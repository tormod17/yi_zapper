{
    "manifest_version": 2,
    "name": "yi-zapper",
    "description": "The zapper MOFO",
    "version": "1.0",
    "browser_action": {
        "default_icon": "images/zapper.png",
        "default_popup": "popup.html",
        "default_title": "zapper"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": false,
        "js":      ["content.js"]
    }],
    "permissions": [
        "activeTab",
        "webRequest",
        "webRequestBlocking"
    ]
}